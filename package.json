{
  "main": "frame.html",
  "name": "Lampa",
  "description": "Chromium Lampa client with AC-3 / DTS support",
  "version": "1.9.5",
  "node-remote": "*://*/*",
  "window": {
    "icon": "frame/icon.png",
    "show": false,
    "frame": false,
    "title": "",
    "toolbar": false,
    "width": 1200,
    "height": 700,
    "min_width": 1000,
    "min_height": 560
  },
  "icons": {
    "256": "frame/icon.png"
  },
  "chromium-args": "--disable-web-security --disable-site-isolation-trials --disable-features=BlockInsecurePrivateNetworkRequests,AsyncDns,UseDnsHttpsSvcbAlpn --enable-experimental-web-platform-features --enable-inband-text-tracks --enable-logging=stderr --enable-webassembly --enable-webgl --ignore-gpu-blacklist"
}
